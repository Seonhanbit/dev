package algo_sub;

import java.io.BufferedReader;
import java.io.InputStreamReader;
import java.util.StringTokenizer;

public class intermediate {
	public static void main(String args[]) throws Exception {
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
		int T = Integer.parseInt(br.readLine());
		for (int tc = 1; tc <= T; tc++) {
			int N = Integer.parseInt(br.readLine());
			char[][] arr = new char[N][N];
			for (int i = 0; i < N; i++) {
				String str = br.readLine();
				for (int j = 0; j < N; j++) {
					arr[i][j] = str.charAt(j);
				}
			}
//			int[] ax = { -1, 1, 0, 0 };
//			int[] ay = { 0, 0, -1, 1 };
//			int[] bx = { -1, -2, 1, 2, 0, 0, 0, 0 };
//			int[] by = { 0, 0, 0, 0, -1, -2, 1, 2 };
//			int[] cx = { -1, -2, -3, 1, 2, 3, 0, 0, 0, 0, 0, 0 };
//			int[] cy = { 0, 0, 0, 0, 0, 0, -1, -2, -3, 1, 2, 3 };
//			arr[i][j]가 A일때, arr[i][j] - 'A' +1 = 1

			int cnt = 0;
			for (int i = 0; i < N; i++) {
				for (int j = 0; j < N; j++) {
					if (arr[i][j] != 'X' && arr[i][j] != 'H') {
//						System.out.println(i+" "+j+" "+(arr[i][j]-'A' +1));
//						만약 A라면 
						if (arr[i][j] == 'A') {
							for (int k = 1; k <= arr[i][j] - 'A' + 1; k++) {
								if(arr[i-k][j]>=0&&arr[i-k][j]=='H')
									arr[i-k][j] = 'X';
								if(arr[i+k][j]<N&&arr[i-k][j]=='H')
									arr[i+k][j] = 'X';
								if(arr[i][j-k]>=0&&arr[i][j-k]=='H')
									arr[i][j-k] = 'X';
								if(arr[i][j+k]<N&&arr[i][j+k]=='H')
									arr[i][j+k] = 'X';
							}
						} else if (arr[i][j] == 'B') {
							for (int k = 1; k <= arr[i][j] - 'A' + 2; k++) {

							}
						} else if (arr[i][j] == 'C') {
							for (int k = 1; k <= arr[i][j] - 'A' + 3; k++) {

							}
						}
					}
				}
			}
		}
	}
}
