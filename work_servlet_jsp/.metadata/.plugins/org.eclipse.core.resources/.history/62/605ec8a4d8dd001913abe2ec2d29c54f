package edu.ssafy.service;

import java.io.IOException;
import java.util.ArrayList;

import javax.servlet.ServletException;
import javax.servlet.http.HttpServletRequest;
import javax.servlet.http.HttpServletResponse;

import edu.ssafy.model.ProductVO;

public interface IProductService {
	private void delPro(HttpServletRequest request, HttpServletResponse response) throws IOException;

	private void viewPro(HttpServletRequest request, HttpServletResponse response)
			throws ServletException, IOException;

	private void addProForm(HttpServletRequest request, HttpServletResponse response)
			throws ServletException, IOException;

	private void addPro(HttpServletRequest request, HttpServletResponse response) throws IOException;

	private void listPro(HttpServletRequest request, HttpServletResponse response)
			throws ServletException, IOException;
}
