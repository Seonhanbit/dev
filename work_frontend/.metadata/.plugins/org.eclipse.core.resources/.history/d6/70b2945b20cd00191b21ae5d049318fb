<script type="text/javascript">

var student = {
		name:"hong"
}
function edit(age,weight){
	this.age=age;
	this.weight=weight}

edit.bind(student)("100","200");
console.log(student);
console.log(student.name);
console.log(student.age);
console.log(student.weight);

</script>
