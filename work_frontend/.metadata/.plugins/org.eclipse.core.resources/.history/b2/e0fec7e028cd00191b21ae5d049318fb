function hello(){
	var i = 10;
	console.log
	console.log(typeof(i));
	var str = 'string';
	var obj = {'name':'hong', 'age':'21'}; 
	//var obj = {}; 오브젝트 만드는 법 2가지
	//var obj = new Object();
	j = 20;
	i = j + str; // “20string”
	console.log(typeof(i));
	if(obj){   // true
		 console.log(obj);
	}
	
	var sin= Math.sin("1.45");
	var sum = 10 + true;  			//11
	var done = sum > "10"; 			//true
	var total = sum - "3";  		//8
	console.log("sin:"+sin + "sum:"+ sum + "done:"+ done+"total:" + total);
	var total = sum + "3";  		//113
	console.log("total:"+ total);
	if(0 == ""){
		console.log("0 == \"\":"+ true);
	}
	
	if(0 === ""){   //type check
		console.log("0 === \"\":"+ true);
	}else{
		console.log("0 === \"\":"+ false);
	}
};
hello();
