<script type="text/javascript">
	var obj1 = new Object();
	console.log(obj1);
	
	obj1.name = 'lee';
	obj1.age = 22;
	
	var obj2 = new Object();
	console.log(obj2);
	
	Object.prototype.name="lee";
	Object.prototype.age=18;
	console.log(obj1);
	
	
	console.log(obj1.name);
	console.log(obj1.age);
	
	var obj2 = new Object();
	console.log(obj2);
	console.log(obj2.name);
	console.log(obj2.age);
	
	
	var obj2 = {};
	obj2.name = 'kim';
	obj2.age = 21;
	
	var obj3 = {
		name : 'choi',
		age : 23
	}
	
	var sts = "1 + 2 + 4";
	
	var res = window.eval(sts);
	console.log(res);
	
	
	var str1 = "{name : 'choi', age : 23 }";
	console.log("----------------------");
	var ob1 = eval("("+str1+")");
	console.log(ob1.name);
	console.log(ob1.age)
	console.log("----------------------");
	var obj4 = {
		name : 'park',
		age : 22
	}
	
	var name = obj4.name;
	console.log(name);
	console.log(obj4.age);

	var name2 = obj4['name'];
	console.log(name2);
	console.log(obj4['age']);
	
	
	var obj5 = {
		name : 'lee',
		info : function(){
			console.log(this.name);
		}	
	}
	obj5.info();
	
	
</script>
