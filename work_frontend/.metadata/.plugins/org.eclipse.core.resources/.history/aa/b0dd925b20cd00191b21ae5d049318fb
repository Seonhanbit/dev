function aa(){
	function inaa(){
		console.log('inaa');
	}
	return [3, 4];
}

var va  = aa();
console.log(va[0] +" : "+va[1]);

function bb(){
	function inaa(){
		console.log('inaa');
	}
	function inbb(){
		console.log('inbb');
	}
	return [inaa, inbb];
}
var vb = bb();
vb[0]();
vb[1]();


