<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<title>Insert title here</title>
<script type="text/javascript">

var Person1 = {fName:'lee',lName:'sejun'};
var Person2 = {fName:'park',lName:'sangjin'};

function hello(message){
	console.log(message+" "+this.fName+" "+this.lName);	
}

hello.call(Person1,'Hello');
hello.call(Person2,"Hello")

hello.apply(Person1,['Hello']);
hello.apply(Person2,['Hello']);

hello.bind(Person1);
Person1.hello();

</script>
</head>
<body>

</body>
</html>