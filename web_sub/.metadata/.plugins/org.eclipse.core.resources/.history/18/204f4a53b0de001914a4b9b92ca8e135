<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
<script type="text/javascript"
	src="//code.jquery.com/jquery-3.3.1.min.js"></script>
<script type="text/javascript">
	window.onload = function() {
		var d = document.querySelector("#div1");
		console.log(d);
	}
	
	$(document).ready(function(){
		var d = document.querySelector("#div1");
		console.log(d);
	});
	
	$(function(){
		var d = document.querySelector("#div1");
		console.log(d);
	});
	
	$("document").find('#div1');
	
	$(function(){
		var d = $("document").find('#div1');
		console.log(d);
	});
	
</script>
</head>
<body>
	<div id=div1></div>
</body>
